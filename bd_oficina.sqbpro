n index="1" value="47"/><column index="2" value="119"/><column index="3" value="64"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="Producao" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="73"/><column index="2" value="132"/><column index="3" value="94"/><column index="4" value="79"/><column index="5" value="48"/><column index="6" value="79"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">SELECT 
    Cos.NomeCostureiro,
    prod.Quantidade_entregue,
	prod.Status,
    pec.valorPeca,
    (prod.Quantidade_entregue * pec.valorPeca) AS TotalValor
FROM 
    Costureiro AS Cos 
LEFT JOIN 
    Producao AS prod ON Cos.idCostureiro = prod.idCostureiro
LEFT JOIN 
    Pecas AS pec ON prod.idPeca = pec.idPeca;
</sql><current_tab id="0"/></tab_sql></sqlb_project>
